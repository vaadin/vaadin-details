{
  "name": "@vaadin/vaadin-details",
  "version": "1.0.1",
  "description": "vaadin-details",
  "main": "vaadin-details.js",
  "module": "vaadin-details.js",
  "repository": "vaadin/vaadin-details",
  "keywords": [
    "Vaadin",
    "vaadin-details",
    "web-components",
    "web-component",
    "lit-element"
  ],
  "author": "Vaadin Ltd",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vaadin/vaadin-details/issues"
  },
  "homepage": "https://vaadin.com/components",
  "files": [
    "/vaadin-*.d.ts",
    "/vaadin-*.d.ts.map",
    "/vaadin-*.js",
    "/vaadin-*.js.map",
    "/src/",
    "/theme/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "tsc && replace-in-file \"/(import|from)( '[.]+\/)(.*)(?<!.js)(';)/g\" \"\\$1\\$2\\$3.js\\$4\" vaadin-*.{js,d.ts},src*.{js,d.ts},theme/**/*.{js,d.ts} --isRegex --verbose",
    "check:version": "magi check-version",
    "gemini": "gemini update test/visual",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint src/*.ts theme/**/*.ts test/*.ts",
    "lint:js": "eslint src theme test --ext .ts --ignore-path .gitignore",
    "lint:lit": "lit-analyzer src --strict",
    "prestart": "wca analyze src/vaadin-details.ts --format json --outFile custom-elements.json",
    "preversion": "magi update-version",
    "start": "es-dev-server --file-extensions .ts --node-resolve --babel --watch --port 3000 --open",
    "test": "karma start",
    "test:debug": "karma start --single-run false --browsers Chrome",
    "test:sauce": "cross-env TEST_PLATFORM=sauce karma start",
    "test:visual": "npm run build && gemini test test/visual"
  },
  "dependencies": {
    "@vaadin/control-state-mixin": "^0.1.0",
    "@vaadin/element-base": "^0.1.2",
    "@vaadin/themable-element": "^0.1.2",
    "@vaadin/vaadin-lumo-styles": "^1.5.0",
    "@vaadin/vaadin-material-styles": "^1.2.3",
    "@vaadin/vaadin-themable-mixin": "^1.5.2",
    "lit-element": "^2.0.0",
    "lit-html": "^1.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/preset-typescript": "^7.7.0",
    "@open-wc/karma-esm": "^2.9.6",
    "@open-wc/testing-helpers": "^1.3.0",
    "@polymer/iron-test-helpers": "3.0.1",
    "@types/chai": "^4.2.4",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.5.0",
    "@types/sinon-chai": "3.2.3",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "api-viewer-element": "^0.2.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "es-dev-server": "^1.23.0",
    "eslint": "^6.6.0",
    "eslint-config-vaadin": "^0.3.1",
    "gemini": "^7.5.1",
    "gemini-esm": "^0.1.0",
    "gemini-sauce": "^1.0.2",
    "husky": "^3.0.9",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^2.0.2",
    "karma-sinon-chai": "^2.0.2",
    "karma-source-map-support": "^1.4.0",
    "lint-staged": "^9.4.2",
    "lit-analyzer": "^1.1.9",
    "magi-cli": "^0.24.0",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "replace-in-file": "^4.2.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "stylelint": "^11.1.1",
    "stylelint-config-vaadin": "^0.2.1",
    "typescript": "^3.6.4",
    "web-component-analyzer": "^0.1.20"
  }
}
